<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_gravity="center"
    tools:context=".MainActivity">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ImageView
            android:id="@+id/imageView15"
            android:layout_width="421dp"
            android:layout_height="1091dp"
            app:srcCompat="@drawable/etoilefond"
            tools:layout_editor_absoluteX="-13dp"
            tools:layout_editor_absoluteY="-100dp" />

        <FrameLayout
            android:layout_width="408dp"
            android:layout_height="847dp">

            <ImageView
                android:id="@+id/imageView3"
                android:layout_width="138dp"
                android:layout_height="135dp"
                android:visibility="visible"
                app:srcCompat="@drawable/asteroid1" />

            <ImageView
                android:id="@+id/imageView11"
                android:layout_width="137dp"
                android:layout_height="125dp"
                app:srcCompat="@drawable/asteroid2" />

            <ImageView
                android:id="@+id/imageView12"
                android:layout_width="178dp"
                android:layout_height="145dp"
                app:srcCompat="@drawable/asteroid3" />

            <ImageView
                android:id="@+id/imageView13"
                android:layout_width="182dp"
                android:layout_height="194dp"
                app:srcCompat="@drawable/tie" />

            <ImageView
                android:id="@+id/imageView14"
                android:layout_width="159dp"
                android:layout_height="127dp"
                app:srcCompat="@drawable/asteroid4" />
        </FrameLayout>

        <FrameLayout
            android:id="@+id/frameLayout2"
            android:layout_width="408dp"
            android:layout_height="112dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent">

            <ImageView
                android:id="@+id/imageView9"
                android:layout_width="106dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                app:srcCompat="@drawable/pad_exterior" />

            <ImageView
                android:id="@+id/imageView7"
                android:layout_width="73dp"
                android:layout_height="72dp"
                android:layout_gravity="center"
                app:srcCompat="@drawable/pad_center" />
        </FrameLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>